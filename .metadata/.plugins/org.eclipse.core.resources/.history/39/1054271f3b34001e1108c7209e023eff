package com.example.anuspringboot;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.context.ApplicationContext;
import org.springframework.context.support.ClassPathXmlApplicationContext;

import com.example.anuspringboot.entity.Topic;

@SpringBootApplication
public class AnuSpringBootApplication {

	public static void main(String[] args) {
		ApplicationContext ctxt = new ClassPathXmlApplicationContext("classpath*:springconfig.xml");
		
		Topic t1 = (Topic)ctxt.getBean(Topic.class);
		System.out.println(t1);
	}

}
